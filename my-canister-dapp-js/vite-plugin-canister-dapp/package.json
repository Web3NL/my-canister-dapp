{
  "name": "@web3nl/vite-plugin-canister-dapp",
  "version": "0.0.0",
  "description": "Vite plugin for Internet Computer canister dapp development configuration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "vite": "^6.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "typecheck": "tsc --noEmit",
    "docs:build": "typedoc",
    "docs:clean": "rm -rf docs",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Web3NL/my-canister-dapp.git",
    "directory": "my-canister-dapp-js/vite-plugin-canister-dapp"
  },
  "homepage": "https://github.com/Web3NL/my-canister-dapp/tree/main/my-canister-dapp-js/vite-plugin-canister-dapp",
  "bugs": {
    "url": "https://github.com/Web3NL/my-canister-dapp/issues"
  },
  "author": "Samer",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "release-it": "^17.0.0",
    "typedoc": "^0.26.0"
  },
  "keywords": [
    "vite",
    "plugin",
    "internet-computer",
    "dfinity",
    "ic",
    "canister",
    "dapp",
    "web3",
    "development",
    "configuration"
  ]
}