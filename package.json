{
  "dependencies": {
    "@dfinity/utils": "^4.1.0",
    "@icp-sdk/auth": "^5.0.0",
    "@icp-sdk/canisters": "^3.4.0",
    "@icp-sdk/core": "^5.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/node": "^25.0.9",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "knip": "^5.81.0",
    "prettier": "^3.8.0",
    "prettier-plugin-svelte": "^3.4.1",
    "sass": "^1.97.2",
    "svelte": "^5.46.4",
    "svelte-check": "^4.3.5",
    "syncpack": "^13.0.4",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "npm@11.7.0",
  "private": true,
  "scripts": {
    "build": "npm run build --workspaces",
    "build:app": "npm run build --workspace=my-canister-app",
    "build:derive-ii-principal": "cd test-fixtures/ii-principal-derivation && vite build",
    "build:docs": "npm run build:docs:dashboard && npm run build:docs:vite-plugin",
    "build:docs:dashboard": "npm run docs:clean --workspace=@web3nl/my-canister-dashboard && npm run docs:build --workspace=@web3nl/my-canister-dashboard",
    "build:docs:vite-plugin": "npm run docs:clean --workspace=@web3nl/vite-plugin-canister-dapp && npm run docs:build --workspace=@web3nl/vite-plugin-canister-dapp",
    "changeset": "changeset",
    "check": "npm run lint --workspaces && npm run format --workspaces && npm run typecheck --workspaces",
    "deps:check": "syncpack list-mismatches && knip",
    "deps:fix": "syncpack format && npx syncpack fix-mismatches",
    "dev:app": "npm run dev --workspace=my-canister-app",
    "dev:dashboard": "npm run dev --workspace=canister-dashboard-frontend",
    "fix": "npm run lint:fix --workspaces && npm run format:fix --workspaces && npm run typecheck --workspaces",
    "release:publish": "changeset publish && git push && git push --tags",
    "release:version": "changeset version",
    "test:create-ii-account": "playwright test test-fixtures/ii-account/create-ii-account.spec.ts",
    "test:derive-ii-principal": "npm run build:derive-ii-principal && playwright test test-fixtures/ii-principal-derivation/derive-ii-principal.spec.ts",
    "test:e2e": "./scripts/run-test-e2e.sh"
  },
  "type": "module",
  "workspaces": [
    "my-canister-dapp-js/my-canister-dashboard-js",
    "my-canister-dapp-js/vite-plugin-canister-dapp",
    "my-canister-dapp-js/my-canister-installer",
    "my-canister-dapp-js/canister-dashboard-frontend",
    "my-canister-app",
    "examples/my-hello-world/src/my-hello-world-frontend"
  ]
}
